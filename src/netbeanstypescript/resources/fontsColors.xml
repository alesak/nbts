<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">

<fontscolors>
    <fontcolor name="keyword" default="keyword"/>
    <fontcolor name="constant" default="number"/>
    <fontcolor name="operator" default="operator"/>
    <fontcolor name="string" default="string"/>
    <fontcolor name="error" default="error"/>
    <fontcolor name="separator" default="separator"/>
    <fontcolor name="identifier" default="identifier"/>
    <fontcolor name="whitespace" default="whitespace"/>

    <fontcolor name="mod-regexp" foreColor="9933CC" default="regexp" />

    <fontcolor name="mod-method" default="method">
        <font style="bold" />
    </fontcolor>
<!-- from javascript2.editor, unimplemented in nbts
    <fontcolor name="mod-custom1" foreColor="666666">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="mod-custom2" default="keyword"/>
-->
    <fontcolor name="mod-class" default="class">
        <font style="bold" />
    </fontcolor>
    <fontcolor name="mod-field" default="field"/>
    <fontcolor name="mod-global" foreColor="9F0D79"/>

    <fontcolor name="mod-unused" waveUnderlined="999999"/>
<!-- from javascript2.editor, unimplemented in nbts
    <fontcolor name="mod-local-variable" default="identifier"/>
    <fontcolor name="mod-local-variable-declaration" default="identifier"/>
-->
    
    <!-- COMMENTS fonts&colors -->
    <fontcolor name="COMMENT" default="comment"/>
    <fontcolor name="comment-line" default="comment"/>
    <!--<fontcolor name="COMMENT_SHARED_TAG" default="comment"/>-->
    <!--<fontcolor name="COMMENT_NOCODE" default="comment" foreColor="BB9955"/>-->
    <fontcolor name="COMMENT_KEYWORD" default="comment">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="COMMENT_HTML" default="comment" foreColor="9999FF">
        <font style="bold"/>
    </fontcolor>

    <fontcolor name="mod-deprecated" strikeThrough="black"/>
    <fontcolor name="mod-undefined" foreColor="red"/>
</fontscolors>
